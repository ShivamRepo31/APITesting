  "type": "object",
  "properties": {
    "base": {
      "type": "string"
    },
    "date": {
      "type": "string",
      "format": "date"
    },
    "time_last_updated": {
      "type": "integer"
    },
    "rates": {
      "type": "object",
      "additionalProperties": {
        "type": "number"
      }
    }
  },
  "required": [
    "base",
    "date",
    "time_last_updated",
    "rates"
  ]
}